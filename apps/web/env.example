# TalentFlux Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_WS_URL=ws://localhost:8000

# Authentication
NEXT_PUBLIC_AUTH_DOMAIN=your-auth-domain.auth0.com
NEXT_PUBLIC_AUTH_CLIENT_ID=your-auth-client-id
NEXT_PUBLIC_AUTH_AUDIENCE=your-auth-audience

# Analytics (Optional)
NEXT_PUBLIC_ANALYTICS_ID=your-analytics-id
NEXT_PUBLIC_SENTRY_DSN=your-sentry-dsn

# Feature Flags
NEXT_PUBLIC_FEATURE_AI_MATCHING=true
NEXT_PUBLIC_FEATURE_AI_SCREENING=true
NEXT_PUBLIC_FEATURE_MASKED_REVIEW=true
NEXT_PUBLIC_FEATURE_DEI_ANALYTICS=true

# Development
NEXT_PUBLIC_DEBUG=false
NEXT_PUBLIC_ENVIRONMENT=development
